<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="online.klok.mobpos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="24" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.android.tools.fd.runtime.BootstrapApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="online.klok.mobpos.LoginActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="online.klok.mobpos.RegisterActivity" />
        <activity android:name="online.klok.mobpos.AdminAreaActivity" />
        <activity android:name="online.klok.mobpos.ManagerAreaActivity" />
        <activity android:name="online.klok.mobpos.UserAreaActivity" />
        <activity android:name="online.klok.mobpos.BillingActivity" />
        <activity android:name="online.klok.mobpos.ReportsActivity" />
        <activity android:name="online.klok.mobpos.MastersActivity" />
        <activity android:name="online.klok.mobpos.SettingsActivity" />
        <activity android:name="online.klok.mobpos.PayCollectActivity" />
    </application>

</manifest>